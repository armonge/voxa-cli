{
  "name": "voxa-cli",
  "version": "2.2.1",
  "description": "The Voxa CLI tools",
  "bin": {
    "voxa": "./bin/voxa.js"
  },
  "engines": {
    "node": ">=8.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mediarain/voxa-cli.git"
  },
  "scripts": {
    "clean": "rm -rf lib/",
    "prepare": "npm run clean && tsc",
    "test": "mocha",
    "test-ci": "nyc mocha --colors --reporter mocha-jenkins-reporter",
    "report": "nyc report --reporter=json && nyc report --reporter html && nyc report --reporter=lcov && nyc report --reporter=cobertura",
    "lint": "tslint --config tslint.json --project tsconfig.json --exclude \"**/*.json\" --exclude \"templates/*\"",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint-fix": "tslint --fix --config tslint.json --project tsconfig.json --exclude \"**/*.json\" --exclude \"templates/*\"",
    "tsc": "rm -rf lib && tsc",
    "create": "npm run tsc && node bin/voxa.js create",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "author": "Rain Agency <npm@rain.agency> (http://rain.agency)",
  "contributors": [
    "Rommel Berrios <rommel.berrios93@gmail.com>",
    "Christian Torres <chtorrez@gmail.com>"
  ],
  "keywords": [
    "alexa",
    "skills",
    "amazon",
    "actions",
    "dialogflow",
    "google",
    "framework",
    "voice",
    "voxa",
    "app"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/inquirer": "^6.0.3",
    "@types/lodash": "^4.14.149",
    "@types/node-xlsx": "^0.12.1",
    "@types/qs": "^6.9.0",
    "@types/uuid": "^3.4.6",
    "axios": "^0.19.1",
    "bluebird": "^3.7.2",
    "colors": "^1.4.0",
    "commander": "^4.1.0",
    "fs-extra": "^8.0.1",
    "google-auth-library": "^5.8.0",
    "google-spreadsheet": "^2.0.7",
    "googleapis": "^46.0.0",
    "handlebars": "^4.5.3",
    "inquirer": "^7.0.3",
    "lodash": "^4.17.15",
    "node-xlsx": "^0.15.0",
    "pad": "^3.2.0",
    "path": "^0.12.7",
    "qs": "^6.9.1",
    "rxjs": "^6.5.4",
    "snyk": "^1.277.1",
    "source-map-support": "^0.5.16",
    "uuid": "^3.3.2",
    "xliff": "^4.4.0"
  },
  "devDependencies": {
    "@types/bluebird-global": "^3.5.12",
    "@types/chai": "^4.2.7",
    "@types/chai-fs": "^2.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.4",
    "@types/simple-mock": "^0.8.1",
    "chai": "^4.2.0",
    "chai-fs": "^2.0.0",
    "husky": "^4.0.1",
    "mocha": "^7.0.0",
    "mocha-jenkins-reporter": "^0.4.1",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "simple-mock": "^0.8.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "^3.7.4"
  },
  "bugs": {
    "url": "https://github.com/mediarain/voxa-cli/issues"
  },
  "homepage": "https://github.com/mediarain/voxa-cli#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint && yarn tsc && yarn test"
    }
  },
  "snyk": true
}
